<div class="page-header">
  <div class="pull-right">

    <%= link_to "Back", company_path(@company), class: "btn btn-info" %>
  </div>
  <h1><%= @location.city %></h1>

</div>


<dl class="dl-horizontal">
  <dt>City</dt>
  <dd><%= @location.city %></dd>
  <dt>Street Name</dt>
  <dd><%= @location.street_name %></dd>
  <dt>State</dt>
  <dd><%= @location.state %></dd>
  <dt>Zipcode</dt>
  <dd><%= @location.zip_code %></dd>
  <dt>Country</dt>
  <dd><%= @location.country %></dd>
</dl>
